{
    "name":        "adrenalinkin/entity-helper-bundle",
    "description": "This bundle provides help for the work with entities which managed by Doctrine",
    "type":        "symfony-bundle",
    "keywords":    ["doctrine", "entity", "helper", "symfony2", "bundle"],
    "license":     "MIT",

    "authors": [
        {
            "email": "adrenalinkin@gmail.com",
            "name":  "Viktor Linkin",
            "role":  "Developer"
        }
    ],

    "require": {
        "php":                          ">=5.4",
        "symfony/config":               "~2.5,<2.7.0",
        "symfony/dependency-injection": "~2.5,<2.7.0",
        "symfony/http-kernel":          "~2.5,<2.7.0"
    },

    "suggest": {
        "symfony/symfony": "Allows more advanced functionality with full Symfony package"
    },

    "autoload": {
        "psr-4": {
            "Linkin\\Bundle\\EntityHelperBundle\\": ""
        }
    }
}
